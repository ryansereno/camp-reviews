<% if (success && success.length){%>

<div name="alert" id="alert" class="alert alert-success alert-dismissible fade show" role="alert">
    <%=success%>
  <button type="button" class="btn btn-close" data-dismiss="alert" aria-label="Close">
  </button>
</div>

<%}%>


<% if (error  && error.length){%>

<div name="alert" id="alert" class="alert alert-danger alert-dismissible fade show" role="alert">
    <%=error%>
  <button type="button" class="btn btn-close" data-dismiss="alert" aria-label="Close">
  </button>
</div>

<%}%>
